
# Copy to .env.ironclaw-aws and fill values before running scripts/ironclaw-aws-stack.sh.

# Public DNS + TLS (A/AAAA record must point to the EC2 instance)
IRONCLAW_DOMAIN=ironclaw.example.com
ACME_EMAIL=ops@example.com

# Postgres
PG_USER=ironclaw
PG_PASSWORD=replace_with_strong_password
PG_DB=ironclaw

# IronClaw security + runtime
SECRETS_MASTER_KEY=replace_with_64_hex
HTTP_WEBHOOK_SECRET=replace_with_hex
NEARAI_SESSION_TOKEN=replace_with_valid_nearai_session_token
RUST_LOG=ironclaw=info

# Optional
OPENAI_API_KEY=
IRONCLAW_SKIP_SESSION_CHECK=false
IRONCLAW_GATEWAY_ENABLED=false
GATEWAY_AUTH_TOKEN=
