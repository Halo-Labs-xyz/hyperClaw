# Copy to .env.docker and fill values before running docker compose.

# --- Ports ---
HYPERCLAW_PORT=3014
IRONCLAW_PORT=8080
PG_PORT=5432

# --- Postgres ---
PG_USER=ironclaw
PG_PASSWORD=ironclaw
PG_DB=ironclaw

# --- hyperClaw auth + security ---
HYPERCLAW_API_KEY=replace_with_hex
MCP_API_KEY=replace_with_hex
ORCHESTRATOR_SECRET=replace_with_hex
SECRETS_MASTER_KEY=replace_with_64_hex
ACCOUNT_ENCRYPTION_KEY=replace_with_64_hex
HCLAW_POINTS_CLOSE_KEY=replace_with_hex

# --- IronClaw sidecar ---
HTTP_WEBHOOK_SECRET=replace_with_hex
IRONCLAW_WEBHOOK_SECRET=replace_with_hex
IRONCLAW_WEBHOOK_URL=http://ironclaw:8080/webhook
NEARAI_SESSION_TOKEN=replace_with_valid_nearai_session_token
# Session check is skipped by default so the stack starts without NEAR AI. Set to false to require a valid session file.
# IRONCLAW_SKIP_SESSION_CHECK=false

# --- Required public app config (baked into Next.js build) ---
NEXT_PUBLIC_PRIVY_APP_ID=replace_with_privy_app_id
NEXT_PUBLIC_PRIVY_CLIENT_ID=replace_with_privy_client_id
NEXT_PUBLIC_WEB_PUSH_PUBLIC_KEY=replace_with_vapid_public_key
NEXT_PUBLIC_EVM_TESTNET=true
NEXT_PUBLIC_MONAD_TESTNET=true
NEXT_PUBLIC_HYPERLIQUID_TESTNET=true
NEXT_PUBLIC_VAULT_ADDRESS=0x0000000000000000000000000000000000000000
NEXT_PUBLIC_MIN_MON_DEPOSIT=450
NEXT_PUBLIC_MAINNET_VAULT_HOTFIX_ADDRESS=
NEXT_PUBLIC_HCLAW_TOKEN_ADDRESS=
NEXT_PUBLIC_HCLAW_LOCK_ADDRESS=
NEXT_PUBLIC_HCLAW_POLICY_ADDRESS=
NEXT_PUBLIC_HCLAW_REWARDS_ADDRESS=
NEXT_PUBLIC_AGENTIC_LP_VAULT_ADDRESS=
# Optional network-specific client overrides
NEXT_PUBLIC_HCLAW_TOKEN_ADDRESS_MAINNET=
NEXT_PUBLIC_HCLAW_TOKEN_ADDRESS_TESTNET=
NEXT_PUBLIC_MONAD_MAINNET_HCLAW_TOKEN_ADDRESS=
NEXT_PUBLIC_MONAD_TESTNET_HCLAW_TOKEN_ADDRESS=
NEXT_PUBLIC_HCLAW_LOCK_ADDRESS_MAINNET=
NEXT_PUBLIC_HCLAW_LOCK_ADDRESS_TESTNET=
NEXT_PUBLIC_MONAD_MAINNET_HCLAW_LOCK_ADDRESS=
NEXT_PUBLIC_MONAD_TESTNET_HCLAW_LOCK_ADDRESS=
NEXT_PUBLIC_HCLAW_POLICY_ADDRESS_MAINNET=
NEXT_PUBLIC_HCLAW_POLICY_ADDRESS_TESTNET=
NEXT_PUBLIC_MONAD_MAINNET_HCLAW_POLICY_ADDRESS=
NEXT_PUBLIC_MONAD_TESTNET_HCLAW_POLICY_ADDRESS=
NEXT_PUBLIC_HCLAW_REWARDS_ADDRESS_MAINNET=
NEXT_PUBLIC_HCLAW_REWARDS_ADDRESS_TESTNET=
NEXT_PUBLIC_MONAD_MAINNET_HCLAW_REWARDS_ADDRESS=
NEXT_PUBLIC_MONAD_TESTNET_HCLAW_REWARDS_ADDRESS=
NEXT_PUBLIC_AGENTIC_LP_VAULT_ADDRESS_MAINNET=
NEXT_PUBLIC_AGENTIC_LP_VAULT_ADDRESS_TESTNET=
NEXT_PUBLIC_MONAD_MAINNET_AGENTIC_LP_VAULT_ADDRESS=
NEXT_PUBLIC_MONAD_TESTNET_AGENTIC_LP_VAULT_ADDRESS=

# --- Runtime server config ---
WEB_PUSH_EMAIL=user@example.com
WEB_PUSH_PRIVATE_KEY=replace_with_vapid_private_key
HYPERLIQUID_PRIVATE_KEY=replace_with_operator_private_key
EVM_PRIVATE_KEY=replace_with_evm_private_key
MONAD_PRIVATE_KEY=replace_with_monad_private_key
OPENAI_API_KEY=replace_with_openai_api_key
MAINNET_BRIDGE_ENABLED=false
MAINNET_MON_MIN_PRICE_USD=1.05
MAINNET_MON_MAX_PRICE_USD=100000
MAINNET_MON_ORACLE_FEED_MAX_DRIFT_PCT=35
# MAINNET_MON_PRICE_OVERRIDE_USD=7.5
MONAD_MAINNET_HCLAW_TOKEN_ADDRESS=
MONAD_TESTNET_HCLAW_TOKEN_ADDRESS=
MONAD_MAINNET_HCLAW_LOCK_ADDRESS=
MONAD_TESTNET_HCLAW_LOCK_ADDRESS=
MONAD_MAINNET_HCLAW_POLICY_ADDRESS=
MONAD_TESTNET_HCLAW_POLICY_ADDRESS=
