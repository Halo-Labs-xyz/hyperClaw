# Copy to .env.docker and fill values before running docker compose.

# --- Ports ---
HYPERCLAW_PORT=3014
IRONCLAW_PORT=8080
PG_PORT=5432

# --- Postgres ---
PG_USER=ironclaw
PG_PASSWORD=ironclaw
PG_DB=ironclaw

# --- hyperClaw auth + security ---
HYPERCLAW_API_KEY=replace_with_hex
MCP_API_KEY=replace_with_hex
ORCHESTRATOR_SECRET=replace_with_hex
SECRETS_MASTER_KEY=replace_with_64_hex
ACCOUNT_ENCRYPTION_KEY=replace_with_64_hex
HCLAW_POINTS_CLOSE_KEY=replace_with_hex

# --- IronClaw sidecar ---
HTTP_WEBHOOK_SECRET=replace_with_hex
IRONCLAW_WEBHOOK_SECRET=replace_with_hex
IRONCLAW_WEBHOOK_URL=http://ironclaw:8080/webhook
NEARAI_SESSION_TOKEN=replace_with_valid_nearai_session_token
# Session check is skipped by default so the stack starts without NEAR AI. Set to false to require a valid session file.
# IRONCLAW_SKIP_SESSION_CHECK=false

# --- Required public app config (baked into Next.js build) ---
NEXT_PUBLIC_PRIVY_APP_ID=replace_with_privy_app_id
NEXT_PUBLIC_PRIVY_CLIENT_ID=replace_with_privy_client_id
NEXT_PUBLIC_WEB_PUSH_PUBLIC_KEY=replace_with_vapid_public_key
NEXT_PUBLIC_MONAD_TESTNET=true
NEXT_PUBLIC_HYPERLIQUID_TESTNET=true
NEXT_PUBLIC_VAULT_ADDRESS=0x0000000000000000000000000000000000000000

# --- Runtime server config ---
WEB_PUSH_EMAIL=user@example.com
WEB_PUSH_PRIVATE_KEY=replace_with_vapid_private_key
HYPERLIQUID_PRIVATE_KEY=replace_with_operator_private_key
MONAD_PRIVATE_KEY=replace_with_monad_private_key
OPENAI_API_KEY=replace_with_openai_api_key
GEMINI_API_KEY=replace_with_google_ai_studio_api_key
NVIDIA_API_KEY=replace_with_nvidia_api_key
GEMINI_MODELS=gemini-3-flash-preview,gemini-2.5-flash
NVIDIA_MODELS=moonshotai/kimi-k2.5,nvidia/llama-3.1-nemotron-ultra-253b-v1
AI_MODEL_CHAIN=gemini:gemini-2.5-flash,nvidia:moonshotai/kimi-k2.5,gemini:gemini-3-flash-preview,nvidia:nvidia/llama-3.1-nemotron-ultra-253b-v1,openai:gpt-4o

# --- Optional storage backends ---
SUPABASE_URL=
SUPABASE_SERVICE_ROLE_KEY=
AWS_S3_BUCKET=
AWS_REGION=
AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
